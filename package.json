{
    "name": "nativescript-tween",
    "version": "1.0.0",
    "scripts": {
        "tsc": "cp src/tween.d.ts plugin && tsc -skipLibCheck -d",

        "build": " cp README.md plugin/ && rm -f .tsbuildinfo && npm run tsc && npm run build.esm && node ./build.esm.js",
        "build.esm": "tsc -skipLibCheck --target es2015 --removeComments --outDir build/esm",
        "demo.ios": "pnpm i && npm run tsc && cd demo && tns run ios",
        "demo.android": "pnpm i && npm run tsc && cd demo && tns run android",
        "clean.demo": "rimraf demo/hooks demo/node_modules demo/platforms",
        "clean": "rimraf plugin/ios plugin/android plugin/**/*.d.ts plugin/**/*.js plugin/**/*.js.map plugin/node_modules plugin/package-lock.json",
        "plugin.watch.tsc": "npm run tsc -- -w",
        "plugin.watch.android": "npm i && npm-watch build.android",
        "plugin.watch.ios": "pnpm i && npm-watch build.ios",
        "plugin.watch": "npm run plugin.watch.tsc & npm run plugin.watch.android & npm run plugin.watch.ios",
        "publish": "npm run build && lerna publish",
        "commitmsg": "commitlint -e $GIT_PARAMS"
    },
    "dependencies": {
        "@tweenjs/tween.js": "18.5.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^9.0.0",
        "@commitlint/config-conventional": "^9.0.0",
        "@types/node": "^14.0.4",
        "@types/tween.js": "^18.5.1",
        "husky": "^4.2.5",
        "lerna": "^3.21.0",
        "npm-watch": "^0.6.0",
        "prompt": "^1.0.0",
        "recursive-copy": "^2.0.10",
        "rimraf": "^3.0.2",
        "through2": "^3.0.1",
        "@nativescript/core": "6.5.1",
        "tns-platform-declarations": "^6.5.1",
        "tslint": "^6.1.2",
        "tslint-config-prettier": "^1.18.0",
        "tslint-config-standard": "^9.0.0",
        "typescript": "~3.9.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/farfromrefug/nativescript-tween.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/farfromrefug/nativescript-tween/issues"
    },
    "homepage": "https://github.com/farfromrefug/nativescript-tween#readme",
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    }
}
