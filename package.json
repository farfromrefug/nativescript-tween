{
    "name": "nativescript-tween",
    "version": "1.0.0",
    "scripts": {
        "tsc": "cp src/tween.d.ts plugin && cp README.md plugin && tsc -skipLibCheck",
        "build": "pnpm i && npm run tsc",
        "demo.ios": "pnpm i && npm run tsc && cd demo && tns run ios",
        "demo.android": "pnpm i && npm run tsc && cd demo && tns run android",
        "clean.demo": "rimraf demo/hooks demo/node_modules demo/platforms",
        "clean": "rimraf plugin/ios plugin/android plugin/**/*.d.ts plugin/**/*.js plugin/**/*.js.map plugin/node_modules plugin/package-lock.json",
        "plugin.watch.tsc": "npm run tsc -- -w",
        "plugin.watch.android": "npm i && npm-watch build.android",
        "plugin.watch.ios": "pnpm i && npm-watch build.ios",
        "plugin.watch": "npm run plugin.watch.tsc & npm run plugin.watch.android & npm run plugin.watch.ios",
        "publish": "npm run build && lerna publish",
        "commitmsg": "commitlint -e $GIT_PARAMS"
    },
    "dependencies": {
        "@tweenjs/tween.js": "18.3.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.2.0",
        "@commitlint/config-conventional": "^8.2.0",
        "@types/node": "^12.7.12",
        "@types/tween.js": "^17.2.0",
        "husky": "^3.0.9",
        "lerna": "^3.17.0",
        "npm-watch": "^0.6.0",
        "prompt": "^1.0.0",
        "rimraf": "^3.0.0",
        "tns-core-modules": "6.1.1",
        "tns-platform-declarations": "^6.1.1",
        "tslint": "^5.20.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-config-standard": "^8.0.1",
        "typescript": "~3.6.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Akylas/nativescript-tween.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Akylas/nativescript-tween/issues"
    },
    "homepage": "https://github.com/Akylas/nativescript-tween#readme",
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    }
}
