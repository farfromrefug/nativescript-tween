{
    "name": "@nativescript-community/tween",
    "version": "0.0.12",
    "description": "A NativeScript plugin that natively wraps tween.js, a tweening engine for easy animations, incorporating optimised Robert Penner's equations",
    "main": "./tween",
    "sideEffects": false,
    "typings": "./tween.d.ts",
    "nativescript": {
        "platforms": {
            "android": "6.0.0",
            "ios": "6.0.0"
        }
    },
    "dependencies": {
        "@tweenjs/tween.js": "18.6.0"
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS",
        "tweens",
        "tween",
        "animation",
        "animations",
        "tween.js"
    ],
    "author": {
        "name": "Martin Guillon",
        "email": "martin@akylas.fr"
    },
    "bugs": {
        "url": "https://github.com/nativescript-community/tween/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/nativescript-community/tween",
    "readmeFilename": "README.md"
}
